<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="viewmodel"
            type="com.example.nucaction.models.WorkflowModel" />
    </data>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp"
        android:background="@color/white"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:padding="10dp"
            android:paddingTop="20dp"
            tools:context=".activities.MainActivity">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:padding="10dp"
                android:paddingTop="20dp">

                <!--title-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/home_row_margin"
                    android:fontFamily="@font/work_sans"
                    android:includeFontPadding="false"
                    android:text="@string/home_welcome"
                    android:textColor="@color/primary_text_color"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/btn_paddingTopBottom"
                    android:fontFamily="@font/work_sans"
                    android:includeFontPadding="false"
                    android:text="@string/home_title"
                    android:textColor="@color/primary_text_color"
                    android:textSize="@dimen/lets_get_started_text_size"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/btnDatePicker"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@drawable/home_datepicker_background"
                    android:fontFamily="@font/work_sans"
                    android:foreground="?attr/selectableItemBackground"
                    android:gravity="center"
                    android:paddingTop="@dimen/btn_paddingTopBottom"
                    android:paddingBottom="@dimen/btn_paddingTopBottom"
                    android:text="@string/select_date"
                    android:textColor="@color/black"
                    android:textSize="@dimen/btn_text_size" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/home_card_kcal_margin"
                    android:layout_marginBottom="@dimen/home_card_kcal_margin"
                    android:elevation="@dimen/card_view_elevation"
                    app:cardCornerRadius="@dimen/card_view_corner_radius">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:padding="@dimen/card_view_layout_content_padding">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:contentDescription="@string/image_contentDescription"
                                android:src="@drawable/ic_home_fire" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/home_text_marginTop"
                                android:gravity="center"
                                android:text="@string/home_subtitle_nutrition_cal"
                                android:textSize="@dimen/home_subtitle_textSize" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="2.5"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/work_sans"
                                android:includeFontPadding="false"
                                android:text="300"
                                android:textColor="@color/black"
                                android:textSize="35dp" />

                            <ProgressBar
                                android:id="@+id/progress_bar_energy"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                android:indeterminate="false"
                                android:max="2000"
                                android:minWidth="200dp"
                                android:minHeight="15dp"
                                android:progress="1" />
                        </LinearLayout>
                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <Button
                    android:id="@+id/btnFindNutrition"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/get_your_nutrition" />

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <!--Row1-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginBottom="@dimen/home_row_margin"
                            android:layout_weight="1"
                            android:background="@drawable/home_cards_db_danger"
                            android:orientation="horizontal"
                            android:padding="@dimen/home_row_padding">

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="117dp"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_vegetable" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_dietary_fiber"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_dietary_fiber"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_dietary_fiber"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                </LinearLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_protein" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_protein"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_protein"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="60"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_protein"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_carbohydrate" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_carbohydrate"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_carbohydrate"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_carbohydrate"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </LinearLayout>
                        <!--Row2-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginBottom="@dimen/home_row_margin"
                            android:layout_weight="1"
                            android:background="@drawable/home_cards_bg_good"
                            android:orientation="horizontal"
                            android:padding="@dimen/home_row_padding">

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius"
                                tools:targetApi="lollipop">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_sugar" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_sugars"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_sugar"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_sugar"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_cholesterol" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_Cholesterol"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_cholesterol"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="10"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_cholesterol"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_na" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_na"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_na"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_na"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </LinearLayout>
                        <!--Row3-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginBottom="@dimen/home_row_margin"
                            android:layout_weight="1"
                            android:background="@drawable/home_cards_db_danger"
                            android:orientation="horizontal"
                            android:padding="@dimen/home_row_padding">

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_oil" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_fat"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_fat"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_fat"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_butter" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_saturated_fatty_acids"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_saturated_fatty_acids"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_saturated_fatty_acids"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/home_subtitle_card_margin"
                                android:layout_weight="1"
                                android:elevation="@dimen/card_view_elevation"
                                app:cardCornerRadius="@dimen/card_view_corner_radius">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    android:padding="@dimen/card_view_layout_content_padding">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/image_contentDescription"
                                        android:src="@drawable/ic_home_danger" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/home_text_marginTop"
                                        android:gravity="center"
                                        android:text="@string/home_subtitle_nutrition_trans_fatty_acids"
                                        android:textSize="@dimen/home_subtitle_textSize" />

                                    <ProgressBar
                                        android:id="@+id/progress_bar_trans_fatty_acids"
                                        style="?android:attr/progressBarStyleHorizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="@dimen/home_progress_bar_marginTop"
                                        android:indeterminate="false"
                                        android:max="24"
                                        android:minWidth="50dp"
                                        android:minHeight="15dp"
                                        android:progress="1" />

                                    <TextView
                                        android:id="@+id/progress_bar_text_trans_fatty_acids"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="@string/home_progress_text"
                                        android:textSize="@dimen/home_subtitle_textSize" />
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </LinearLayout>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
        </LinearLayout>

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:id="@+id/camera_bottom_navigation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/white"
            app:menu="@menu/bottom_navigation"
            app:itemIconTint="@drawable/camera_nevigator_view_sel"
            >
        </com.google.android.material.bottomnavigation.BottomNavigationView>


    </RelativeLayout>
</layout>